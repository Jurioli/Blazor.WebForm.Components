@using Blazor.WebForm.UI
@using Blazor.WebForm.UI.ControlComponents
@using System.Web.UI
@using System.Web.UI.WebControls
@inherits ControlComponentBase<System.Web.UI.WebControls.VirtualizePlaceHolder>
@this.Render(this.Control)
@code {
    [Parameter]
    public string SpacerElement
    {
        get
        {
            return this.Control.SpacerElement;
        }
        set
        {
            this.Control.SpacerElement = value;
        }
    }

    [Parameter]
    public float? ItemSize
    {
        get
        {
            return this.Control.ItemSize;
        }
        set
        {
            this.Control.ItemSize = value;
        }
    }

    [Parameter]
    public int? OverscanCount
    {
        get
        {
            return this.Control.OverscanCount;
        }
        set
        {
            this.Control.OverscanCount = value;
        }
    }

    [Parameter]
    public int? MaxItemCount
    {
        get
        {
            return this.Control.MaxItemCount;
        }
        set
        {
            this.Control.MaxItemCount = value;
        }
    }
}
