@using Blazor.WebForm.UI;
@using Blazor.WebForm.UI.ControlComponents;
@using System.Web.UI;
@using System.Web.UI.WebControls;
@inherits BaseValidatorControlComponent<System.Web.UI.WebControls.RangeValidator>
@this.RenderWithInner(this.Control)
@code {
    [Parameter]
    public string MaximumValue
    {
        get
        {
            return this.Control.MaximumValue;
        }
        set
        {
            this.Control.MaximumValue = value;
        }
    }

    [Parameter]
    public string MinimumValue
    {
        get
        {
            return this.Control.MinimumValue;
        }
        set
        {
            this.Control.MinimumValue = value;
        }
    }
}
